{
  "name": "eslint-plugin-restrict-replace-import",
  "version": "2.0.0",
  "description": "ESLint Plugin for Restricting and Replacing Import",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "import",
    "restrict"
  ],
  "author": "shiwoo.park",
  "bugs": {
    "url": "https://github.com/custardcream98/eslint-plugin-restrict-replace-import/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/custardcream98/eslint-plugin-restrict-replace-import.git"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "prettier --write .",
    "lint": "pnpm lint:eslint-docs && pnpm lint:js",
    "lint:eslint-docs": "eslint-doc-generator --check",
    "lint:js": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "update:eslint-docs": "eslint-doc-generator",
    "prepublishOnly": "pnpm build",
    "release": "pnpm lint && pnpm test && pnpm publish"
  },
  "packageManager": "pnpm@10.27.0",
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@types/eslint": "^9.6.1",
    "@types/estree": "^1.0.6",
    "@types/node": "^22.10.5",
    "eslint": "^9.22.0",
    "eslint-doc-generator": "^2.1.0",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0",
    "vitest": "^3.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "peerDependencies": {
    "eslint": ">=8"
  },
  "license": "ISC"
}
